<?php
    $hostname="localhost";
    $username1="root";
    $password1="";
    $database="toko_online";

    $koneksi=mysqli_connect ($hostname, $username1,$password1, $database);
    
    if($koneksi){
        echo "";
    }
    else{
        die("koneksi ke database gagal").mysqli_connect_error();
    }
?>
